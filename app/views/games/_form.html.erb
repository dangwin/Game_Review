

<%= simple_form_for @game do |f| %>
<%= f.input :title, label: "Game Title" %>
<%= f.input :platform %>
<%= f.input :description %>
<%= f.input :genre %>
<%= f.hidden_field :creator_id%>
<%= f.button :submit %>

<br><br>

<%= link_to "Home", user_path(current_user) %><br><br>

<%= link_to "View All Games", games_path %><br><br>

<%= link_to "LogOut", logout_url, method: :delete %>

<% end %>